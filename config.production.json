{
  "redis": {
    "url": "redis://redis-server:6379",
    "collection_name": "mcp_memories_prod",
    "db": 0
  },
  "mem0": {
    "llm": {
      "provider": "ollama",
      "config": {
        "model": "llama3.2:3b",
        "temperature": 0.1,
        "max_tokens": 1000,
        "base_url": "http://ollama-server:11434"
      }
    },
    "embedder": {
      "provider": "ollama",
      "config": {
        "model": "nomic-embed-text:latest",
        "base_url": "http://ollama-server:11434"
      }
    },
    "vector_store": {
      "provider": "redis",
      "config": {
        "collection_name": "mcp_memories_prod",
        "embedding_model_dims": 768
      }
    }
  },
  "server": {
    "name": "memory-mcp",
    "version": "1.0.0",
    "log_level": "WARNING"
  }
}